<% include _header %>

<div class="container">
<h2>Our users</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">fname</th>
      <th scope="col">lname</th>
      <th scope="col">email</th>
      <th scope="col">telephone</th>
      <th scope="col">Bio</th>
      <th scope="col">APIkey</th>
      <th scope="col">displayName</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(user) { %>
    <tr>
      <td>
        <a href="/users/<%= user.username %>">
          <%= user.username %>
        </a>
      </td>
      <td>
        <%= user.fname %>
      </td>
      <td>
        <%= user.lname %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.telephone %>
      </td>
      <td>
        <%= user.bio %>
      </td>
      <td>
        <%= user.apiKey %>
      </td>
      <td>
        <%= user.displayName %>
      </td>
      <% }) %>
      </td>
    </tr>
  </tbody>
</table>

<% include _footer %>
</div>